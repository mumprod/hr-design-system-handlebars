{
    "config": {
        "template": "default"
    },
    "metadatabox": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "default"
        }
    },
    "metadatabox_with_comments": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "default",
            "@->extends": {
                "userComments": {
                    "HasOneComment": false,
                    "quantity": "117"
                }
            }
        }
    },
    "metadatabox_with_one_author": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_author"
        }
    },
    "metadatabox_with_one_guest_author": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_guest_author"
        }
    },
    "metadatabox_with_one_author_and_comments": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_author",
            "@->extends": {
                "userComments": {
                    "HasOneComment": false,
                    "quantity": "117"
                }
            }
        }
    },
    "metadatabox_with_one_author_and_without_picture": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_author",
            "@->overrides": [
                {
                    "@->contentpath": "editorialData.firstAuthorItem.profileImage",
                    "@->value": {}
                }
            ]
        }
    },
    "metadatabox_with_more_authors": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_more_authors"
        }
    },
    "metadatabox_with_more_authors_and_comments": {
        "config": {
            "css": "w-[724px] mx-auto"
        },
        "args": {
            "@->jsoninclude": "page/metadatabox.inc.json",
            "@->contentpath": "with_more_authors",
            "@->extends": {
                "userComments": {
                    "HasOneComment": false,
                    "quantity": "117"
                }
            }
        }
    }
}
