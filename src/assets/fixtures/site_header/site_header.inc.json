{
    "default": {
        "hasOpenSubNavigation": false,
        "_useSticky": true,
        "brandNavigationItems": {
            "@->jsoninclude": "site_header/brandNavigation.inc.json"
        },
        "serviceNavigationSSILinks": {
            "@->jsoninclude": "site_header/serviceNavigation.inc.json"
        },
        "sectionNavigationSSILinks": {
            "@->jsoninclude": "site_header/sectionNavigation.inc.json"
        },
        "contentNav": {
            "@->jsoninclude": "teaser/teasers.inc.json",
            "@->contentpath": "teaserContentNav.contentNav",
            "@->overrides": [
                {
                    "@->contentpath": "isDropdown",
                    "@->value": true
                }
            ]
        }
    },
    "with_submenu": {
        "hasOpenSubNavigation": true,
        "_useSticky": true,
        "brandNavigationItems": {
            "@->jsoninclude": "site_header/brandNavigation.inc.json"
        },
        "serviceNavigationSSILinks": {
            "@->jsoninclude": "site_header/serviceNavigation.inc.json"
        },

        "sectionNavigationSSILinks": {
            "@->jsoninclude": "site_header/sectionNavigation_mit_submenu.inc.json"
        }
    },
    "with_submenu_as_flyout": {
        "hasOpenSubNavigation": true,
        "_useSticky": true,
        "brandNavigationItems": {
            "@->jsoninclude": "site_header/brandNavigation.inc.json"
        },
        "serviceNavigationSSILinks": {
            "@->jsoninclude": "site_header/serviceNavigation_hr3.inc.json"
        },

        "sectionNavigationSSILinks": {
            "@->jsoninclude": "site_header/sectionNavigation_mit_submenu_as_flyout.inc.json"
        }
    },
    "with_top_topics": {
        "hasOpenSubNavigation": false,
        "_useSticky": true,
        "brandNavigationItems": {
            "@->jsoninclude": "site_header/brandNavigation.inc.json"
        },
        "serviceNavigationSSILinks": {
            "@->jsoninclude": "site_header/serviceNavigation.inc.json"
        },
        "topTopicsNavigation": {
            "@->jsoninclude": "site_header/topTopicsNavigation.inc.json"
        },
        "sectionNavigationSSILinks": {
            "@->jsoninclude": "site_header/sectionNavigation.inc.json"
        }
    },
    "with_warnings": {
        "hasOpenSubNavigation": false,
        "_useSticky": true,
        "brandNavigationItems": {
            "@->jsoninclude": "site_header/brandNavigation.inc.json"
        },
        "serviceNavigationSSILinks": {
            "@->jsoninclude": "site_header/serviceNavigation_mit_warnung.inc.json"
        },

        "sectionNavigationSSILinks": {
            "@->jsoninclude": "site_header/sectionNavigation.inc.json"
        }
    }
}
