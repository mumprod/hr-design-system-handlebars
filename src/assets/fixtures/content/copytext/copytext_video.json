{
    "copytextParagraph": [
        {
            "isHeadline": true,
            "text": "Copytext mit Video"
        },
        {
            "@->jsoninclude": "content/copytext/copytext_paragraph.inc.json"
        },
        {
            "paragraphBoxItem": {

                        "isVideo": true,
                        "showFullSize": {
                            "isTrue": true
                        },
                        "toModel": {
                            "videoElement": {
                                "ardPlayerConfig": {
                                    "@->jsoninclude": "video/videos.inc.json",
                                    "@->contentpath": "video-1"
                                },
                                "ardPlayerJsonConfig": {
                                    "@->jsoninclude": "video/videos.inc.json",
                                    "@->contentpath": "video-1"
                                }
                            }
                        },
                        "content":{
                            "teaserImage": {
                                "@->jsoninclude": "teaser/teaser_images.inc.json",
                                "@->contentpath": "copytext_image_100",
                                    "@->replaceToken": [
                                        {
                                            "@->token": "image",
                                            "@->value": "lehrerin-ukraine-104"
                                        }
                                    ]
                            }
                        }
            }
        },
        {
            "paragraphBoxItem": {
                "@->jsoninclude": "teaser/teaser_images.inc.json",
                "@->contentpath": "copytext_image_100",
                "@->replaceToken": [
                    {
                        "@->token": "image",
                        "@->value": "buchmesse-fahnen-102"
                    }
                ]
            }
        },
        {
            "paragraphBoxItem": {

                        "isVideo": true,
                        "showFullSize": {
                            "isTrue": false
                        },
                        "toModel": {
                            "videoElement": {
                                "ardPlayerConfig": {
                                    "@->jsoninclude": "video/videos.inc.json",
                                    "@->contentpath": "video-2"
                                },
                                "ardPlayerJsonConfig": {
                                    "@->jsoninclude": "video/videos.inc.json",
                                    "@->contentpath": "video-2"
                                }
                            }
                        },
                        "content":{
                            "teaserImage": {
                                "@->jsoninclude": "teaser/teaser_images.inc.json",
                                "@->contentpath": "copytext_image_100",
                                    "@->replaceToken": [
                                        {
                                            "@->token": "image",
                                            "@->value": "lehrerin-ukraine-104"
                                        }
                                    ]
                            }
                        }
            }
        },
        {
            "@->jsoninclude": "content/copytext/copytext_paragraph.inc.json"
        }
    ]
}
