    "teaser_contentnav": {
        "includePath": "components/teaser/group_teaser/group_teaser_accented",
        "includeModel": {
            "hasLink": false,
            "link": "#gruppe1",
            "hasGroupTitle": true,
            "groupTitle": "Akzentuier-Gruppe 100%, 1 x 100% ContentNav Aufklapp ",
            "teaserSize": "100",
            "isHighlightGroup": false,
            "backgroundColor": "",
            "logicItems": [
                {
                    "isSsi": true,
                    "logicItem": {
                        "includePath": "components/teaser/content_nav/teaser_content_nav",
                        "includeModel": {
                            "@->jsoninclude": "teaser/teasers.inc.json",
                            "@->contentpath": "teaserContentNav",
                            "@->overrides": [
                                {
                                    "@->contentpath": "contentNav.isAutosuggest",
                                    "@->value": true
                                },
                                {
                                    "@->contentpath": "contentNav.isDropdown",
                                    "@->value": true
                                }
                            ]
                        }
                    }
                }
            ]
        }
    },
