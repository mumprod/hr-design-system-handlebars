<div class="js-fr-content">
    {{#each this.months}}
        {{#each this.days}}
            {{#each this.instants}}
                {{#unless this.isDateInstant}}
                    {{> components/event/calendar/event_calendar_event_teaser _showTeaser=../this.isFirstAvailable _withConcertInfo=../../../_withConcertInfo}}
                {{/unless}}
            {{/each}}
        {{/each}}
    {{/each}}
</div>