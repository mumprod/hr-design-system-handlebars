{{~#if _firstPage~}}   
    <div class="flex"> 
        {{> components/pagination/pagination_button_left }}   
        {{#with this}}
            {{#> components/button/link_button _size="md" _variant="primary" _css="mr-1 ml-1 dark:fill-link-dark"}}
                <span class="sr-only">Aktuelle Seite: </span>
                {{> components/button/components/button_label _label="1" _css=""}}
            {{/components/button/link_button}} 
            {{#*inline "htmlProperties"}}
                aria-current="true"
            {{/inline}}  
        {{/with}}
        <span class="pt-1 ml-2 mr-2 font-title dark:text-link-dark">&hellip;</span>
        {{#if _unlinkLastPage}}          
            {{#with this}}
                <span class="text-button-disabled--dark {{> components/button/utilities/button_base_classes}} {{> components/button/utilities/button_dimension_classes _size='md' ~}}">{{ _totalPages-adjust_context}}</span>
            {{/with}}
        {{else}}
            {{#with this}}
                {{#> components/button/link_button _size="md" _variant="secondary" _link=this.lastPageItemLink _css="mr-1 ml-1"}}
                    <span class="sr-only">Zur Seite: </span>
                    {{> components/button/components/button_label _label=_totalPages-adjust_context _css=""}}
                {{/components/button/link_button}}
                {{#*inline "htmlProperties"}}{{/inline}}
            {{/with}}
        {{/if}}
        {{> components/pagination/pagination_button_right_linked }} 
    </div>     
{{else}}   
    {{~#if _secondPage~}}  
    <div class="flex">  
        {{> components/pagination/pagination_button_left_linked }}    
        {{#> components/button/link_button _size="md" _variant="secondary" _link=this.firstPageItemLink _css="mr-1 ml-1"}}
            <span class="sr-only">Zur Seite: </span>
            {{> components/button/components/button_label _label="1" _css=""}}
        {{/components/button/link_button}}  
        {{#with this}}
            {{#> components/button/link_button _size="md" _variant="primary" _css="mr-1 ml-1 dark:fill-link-dark"}}
                <span class="sr-only">Aktuelle Seite: </span>
                {{> components/button/components/button_label _label="2" _css=""}}
            {{/components/button/link_button}} 
            {{#*inline "htmlProperties"}}
                aria-current="true"
            {{/inline}}  
        {{/with}}   
        <span class="pt-1 ml-2 mr-2 font-title dark:text-link-dark">&hellip;</span>
        {{#if _unlinkLastPage}}            
            {{#with this}}
                <span class="text-button-disabled--dark {{> components/button/utilities/button_base_classes}} {{> components/button/utilities/button_dimension_classes _size='md' ~}}">{{ _totalPages-adjust_context}}</span>
            {{/with}}
        {{else}}
            {{#with this}}
                {{#> components/button/link_button _size="md" _variant="secondary" _link=this.lastPageItemLink _css="mr-1 ml-1"}}
                    <span class="sr-only">Zur Seite: </span>
                    {{> components/button/components/button_label _label=_totalPages-adjust_context _css=""}}
                {{/components/button/link_button}}
                {{#*inline "htmlProperties"}}{{/inline}}
            {{/with}}           
        {{/if}}   
        {{> components/pagination/pagination_button_right_linked }} 
    </div>   
    {{else}}
        <div class="flex"> 
            {{> components/pagination/pagination_button_left_linked }}   
            {{#> components/button/link_button _size="md" _variant="secondary" _link=this.firstPageItemLink _css="mr-1 ml-1"}}
                <span class="sr-only">Zur Seite: </span>
                {{> components/button/components/button_label _label="1" _css=""}}
            {{/components/button/link_button}}  
            <span class="pt-1 ml-2 mr-2 font-title dark:text-link-dark">&hellip;</span>  
            {{#with this}} 
                {{#> components/button/link_button _size="md" _variant="primary" _css="mr-1 ml-1 dark:fill-link-dark"}}
                    <span class="sr-only">Aktuelle Seite: </span>
                    {{> components/button/components/button_label _label=_currentPage-adjust_context _css=""}}
                {{/components/button/link_button}}     
                {{#*inline "htmlProperties"}}
                    aria-current="true"
                {{/inline}}  
            {{/with}}  
            {{~#if _notLastPage~}}   
                {{~#if _notLastButOnePage~}}   
                        <span class="pt-1 ml-2 mr-2 font-title dark:text-link-dark">&hellip;</span>   
                {{~/if~}}   
                {{#if _unlinkLastPage}}       
                    {{#with this}}
                        <span class="text-button-disabled--dark {{> components/button/utilities/button_base_classes}} {{> components/button/utilities/button_dimension_classes _size='md' ~}}">{{ _totalPages-adjust_context}}</span>
                    {{/with}}
                {{else}}
                    {{#with this}}
                        {{#> components/button/link_button _size="md" _variant="secondary" _link=this.lastPageItemLink _css="mr-1 ml-1"}}
                            <span class="sr-only">Zur Seite: </span>
                            {{> components/button/components/button_label _label=_totalPages-adjust_context _css=""}}
                        {{/components/button/link_button}}
                        {{#*inline "htmlProperties"}}{{/inline}}
                    {{/with}}                 
                {{/if}}
                
            {{~/if~}}      
            {{~#if _notLastPage~}}   
                {{> components/pagination/pagination_button_right_linked }}  
            {{else}}   
                {{> components/pagination/pagination_button_right }}  
            {{~/if~}} 
         </div>  
    {{~/if~}}   
{{~/if~}}  