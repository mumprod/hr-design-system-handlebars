{{~#with this.logicItem~}}
    {{~#with this.includeModel~}}
        {{~#if (isStorybook)~}}
            {{~> components/base/load_dynamic _templatePath=../includePath ~}}
        {{~else~}}
            {{~#with this.cacheKey~}}
                {{~addCacheEntry this ~}}
            {{~/with~}}
            {{~> (lookup ../. 'includePath')~}}
        {{~/if~}}
    {{~/with~}}
{{~/with~}}